      {
        "pagePath":"pages/nearby/nearby",
        "text":"附近",
        "iconPath": "./assets/images/tabbar/nearby.png",
        "selectedIconPath":"./assets/images/tabbar/nearby-s.png"
      },
      {
        "pagePath":"pages/profile/profile",
        "text":"我的",
        "iconPath": "./assets/images/tabbar/user.png",
        "selectedIconPath":"./assets/images/tabbar/user-s.png"
      }
      
    ]
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
