<view class="img-upload">
  <swiper autoplay="true" indicator-dots>
    <block wx:for="{{swiperImg}}">
      <swiper-item>
        <image src="{{item}}" class="swiper-img"></image>
        <view>{{index}}</view>
      </swiper-item>
    </block>
  </swiper>
  <view bindtap="chooseImage">+</view>
</view>